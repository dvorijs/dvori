{
    "packages": [
        "packages/core",
        "packages/utils",
        "packages/plugins"
    ],
    "version": "1.0.2",
    "exact": true,
    "command": {
        "init": {
            "exact": true
        },
        "publish": {
            "conventionalCommits": true
        },
        "version": {
            "message": "chore(release): publish"
        }
    },
    "changelog": {
        "labels": {
            "feat": "New Feature",
            "fix": "Bug Fix",
            "docs": "Documentation",
            "perf": "Performance",
            "refactor": "Code Refactoring"
        }
    }
}
