{
    "name": "dvori",
    "scope": "@dvori",
    "version": "1.0.0",
    "description": "Composable HTTP client for Node.js",
    "main": "index.js",
    "directories": {
        "packages": "packages"
    },
    "scripts": {
        "test": "npm test --workspaces",
        "test:coverage": "npm test --workspaces -- --coverage",
        "release": "NODE_ENV=production npx np"
    },
    "homepage": "https://dvorijs.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dvorijs/dvori.git"
    },
    "workspaces": [
        "packages/*",
        "core",
        "utils"
    ],
    "bugs": {
        "url": "http://github.com/dvorijs/dvori/issues"
    },
    "engines": {
        "node": ">=8.10.0"
    },
    "np": {
        "yarn": false,
        "cleanup": true
    },
    "keywords": [
        "http",
        "request",
        "rest",
        "http-client",
        "functional",
        "composable",
        "compose",
        "simple",
        "util",
        "utility"
    ],
    "author": "IMNS LLC",
    "license": "MPL-2.0",
    "devDependencies": {
        "jest": "^27.4.3",
        "np": "^7.5.0"
    },
    "dependencies": {
        "debug": "4.3.2",
        "deepmerge": "^4.0.0"
    }
}
