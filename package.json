{
    "name": "dvori",
    "version": "1.0.0",
    "description": "Functional composable HTTP client for Node.js",
    "main": "index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "DEBUG=dvori* npx jest",
        "test:coverage": "npx jest --coverage",
        "release": "NODE_ENV=production npx np"
    },
    "homepage": "https://github.com/readablereport/dvori#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/readablereport/dvori.git"
    },
    "bugs": {
        "url": "http://github.com/readablereport/dvori/issues"
    },
    "engines": {
        "node": ">=8.10.0"
    },
    "np": {
        "yarn": false,
        "cleanup": true
    },
    "keywords": [
        "http",
        "request",
        "rest",
        "http-client",
        "functional",
        "composable",
        "compose",
        "simple",
        "util",
        "utility"
    ],
    "author": "Nathan Smith <nate@readable.report> (https://readable.report)",
    "license": "MPL-2.0",
    "jest": {
        "testMatch": [
            "**/test/**/*.js"
        ],
        "testPathIgnorePatterns": [
            "/test/helpers/",
            "/test/__mocks__/"
        ]
    },
    "devDependencies": {
        "jest": "^27.3.1",
        "np": "^7.5.0"
    },
    "dependencies": {
        "debug": "4.3.2",
        "deepmerge": "^4.0.0"
    }
}
